{
  "AppConfig": {
    "MovieApiBase": "https://localhost:7028/api/",
    "MovieApiVersion": "v1",
    "LoginPath": "/Identity/Login",
    "RegisterPath": "/Identity/Register",
    "RefreshTokenPath": "/Identity/RefreshToken",
    "MovieApiPath": "/Movie/",
    "LogoutPath": "/Identity/Logout",
    "CreateMoviePath": "/Movie/",
    "GetAllMoviesPath": "/Movie/",
    "UpdateMoviePath": "/Movie/",
    "DeleteMoviePath": "/Movie/",
    "GetAllGenres": "/Genre/",
